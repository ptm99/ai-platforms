# Backend (container)
PORT=8080

# IMPORTANT:
# In docker-compose-be.yml, DATABASE_URL is set explicitly via environment:
#   postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgres:5432/${POSTGRES_DB}
# This fallback is only for running backend alone without compose.
DATABASE_URL=postgres://postgres:postgrespw123@postgres:5432/ai_platform

# JWT (compose will override from root .env)
JWT_ACCESS_SECRET=myjwtsecret123
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_SECRET=myjwtsecret123
JWT_REFRESH_EXPIRES_IN=30d

# Encryption (compose will override from root .env)
# Do NOT add inline comments on the same line as the value.
KEY_ENC_SECRET=12345678901234567890123456789012

# CORS
CORS_ORIGIN=*

# Model discovery (compose maps ENABLE_MODEL_DISCOVERY -> MODEL_DISCOVERY_ENABLED)
MODEL_DISCOVERY_ENABLED=false
MODEL_DISCOVERY_CRON=0 * * * *
